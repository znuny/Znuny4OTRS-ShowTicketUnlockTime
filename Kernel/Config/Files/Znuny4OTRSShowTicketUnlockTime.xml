<?xml version="1.0" encoding="utf-8"?>
<otrs_config version="1.0" init="Application">
    <ConfigItem Name="Frontend::Output::FilterElementPost###ZnunyTicketUnlockTime" Required="0" Valid="1">
        <Description Translatable="1">Defines the filter that processes the HTML temlates to add ticket unlock time in ticket zoom.</Description>
        <Group>Znuny4OTRS-ShowTicketUnlockTime</Group>
        <SubGroup>Frontend::Agent::Ticket::ViewZoom</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::FilterElementPost::ZnunyTicketUnlockTime</Item>
                <Item Key="Templates">
                    <Hash>
                        <Item Key="AgentTicketZoom">1</Item>
                    </Hash>
                </Item>
            </Hash>
        </Setting>
    </ConfigItem>
</otrs_config>
